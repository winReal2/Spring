                                    <tr>
                                        <th></th>
                                        <th>도서명</th>
                                        <th>작가</th>
                                        <th>등록일</th>
                                        <th>대여여부</th>
                                    </tr>
                                </thead>
                                <tbody>
                                
                                
                                <c:forEach items="${list }" var="vo">
                                    <tr class="odd gradeX">
                                        <td><input type="checkbox"></td>
                                        <td>${vo.no }</td>
                                        <td>${vo.title }</td>
                                        <td>${vo.author }</td>
                                        <td class="center">${vo.rentynStr }</td>
                                    </tr>
                                    </c:forEach>
                                </tbody>
                            </table>
                            <div class="pageNavi" style="align:center">
                            	<%@include file="../common/pageNavi.jsp" %>
