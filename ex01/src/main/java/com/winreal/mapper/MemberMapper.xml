<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.winreal.mapper.MemberMapper">

	<select id="login" resultType="com.winreal.vo.Member">
		<!-- 파라미터 받을 부분에 #이용 $넣으면 실행되긴하지만 preparedStatment형식으로 하려면 #넣어줘야함 -->
		select * from member where id=#{id}
	</select>
	
	<insert id="insert">
		insert into member (id, pw, name)
					values (#{id}, #{pw}, #{name})
	</insert>
	
	<select id="idCheck" resultType="int">
		select count(*)
			from member 
			where id=#{id}
	</select>
	
	<!-- 권한조회 -->
	<select id="getMemberRole" resultType="String">
		select role_id from memberrole where id=#{id}
	</select>


</mapper>

















